          </div>
        </div>
        <!-- Footer Bottom -->
        <div class="border-t border-gray-600 mt-8 pt-4">
          <div class="flex flex-col md:flex-row justify-between items-center">
            <p>
              © Copyright 2024. All Rights Reserved by
              <a href="#" class="text-primary hover:underline">validthemes</a>
            </p>
            <ul class="flex space-x-4 mt-4 md:mt-0">
              <li>
                <a href="#" class="text-primary hover:underline">Terms</a>
              </li>
              <li>
                <a href="#" class="text-primary hover:underline">Privacy</a>
              </li>
